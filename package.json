{
  "name": "quintype-amp",
  "version": "0.1.0",
  "description": "Quintype's AMP component library for publisher apps to create amp layouts",
  "scripts": {
    "storybook": "start-storybook -p 9001",
    "build": "webpack",
    "test": "jest --env=jsdom --watch",
    "test:cover": "jest --env=jsdom --coverage --watchAll; open coverage/index.html",
    "test:ci": "jest --env=jsdom --coverage --maxWorkers=2",
    "format": "npx prettier '**/*.{js,jsx,ts,tsx,css,scss}' --write",
    "lint": "tslint 'src/**/*.tsx'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quintype/quintype-amp.git"
  },
  "author": "dev-core@quintype.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quintype/quintype-amp/issues"
  },
  "homepage": "https://github.com/quintype/quintype-amp#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/react": "^5.3.13",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^4.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "react-amphtml": "^2.2.2",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "quintype-js": "^1.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
