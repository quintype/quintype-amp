{
  "name": "@quintype/amp",
  "version": "1.1.0-tooling.0",
  "description": "Quintype's AMP component library for publisher apps to create amp layouts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run jest && ./bin-dev-scripts/standard-version-release.sh",
    "storybook": "start-storybook -s ./storybook-assets -p 9001",
    "build": "rimraf dist && webpack",
    "pack": "npm run build && npm pack",
    "dist": "npm run build && npm publish --access public",
    "beta": "npm run build && npm publish --tag beta --access public",
    "test": "jest --env=jsdom --watch",
    "jest": "jest --env=jsdom",
    "test:cover": "jest --env=jsdom --coverage --watchAll; open coverage/index.html",
    "test:ci": "jest --env=jsdom --coverage --maxWorkers=2",
    "format": "npx prettier --write",
    "lint": "tslint 'src/**/*.tsx'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quintype/quintype-amp.git"
  },
  "author": "dev-core@quintype.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quintype/quintype-amp/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run jest"
    }
  },
  "homepage": "https://github.com/quintype/quintype-amp#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react": "^5.3.13",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^4.4.2",
    "@types/webpack": "^4.41.7",
    "amphtml-validator": "^1.0.30",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "lint-staged": "^10.2.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.0",
    "ts-jest": "^25.2.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@rvgpl/get-youtube-id": "^1.0.0",
    "atob": "^2.1.2",
    "date-fns": "^2.11.0",
    "date-fns-tz": "^1.0.10",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "node-html-parser": "^1.2.15",
    "performant-array-to-tree": "^1.7.1",
    "quintype-js": "^1.2.1",
    "react-helmet": "^5.2.1",
    "styled-components": "~5.0.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
